Host *
     StrictHostKeyChecking no
     UserKnownHostsFile=/dev/null
     LogLevel=quiet

Host 10.*
     IdentityFile ~/.ssh/id_rsa
     User tweiss
     ConnectTimeout 20
     ServerAliveInterval 300
     ServerAliveCountMax 2

Host 172.*
     IdentityFile ~/.ssh/id_rsa
     User tweiss
     ConnectTimeout 20
     ServerAliveInterval 300
     ServerAliveCountMax 2

Host *.trade.tt
     IdentityFile ~/.ssh/id_rsa
     User tweiss
     ConnectTimeout 20

Host jenkins*
     IdentityFile ~/.ssh/id_rsa
     User tweiss
     ConnectTimeout 20
